<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.laboratory1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/airplane_ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/airplane_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Laboratory1">
        <activity android:name=".presenter.game.GameActivity"></activity>
        <activity android:name=".presenter.game.SecondActivity" />
        <activity android:name=".presenter.SixthActivity" />
        <activity
            android:name=".presenter.FourthActivity"
            android:parentActivityName=".presenter.ThirdActivity" />
        <activity android:name=".presenter.FifthActivity" />
        <activity android:name=".presenter.ThirdActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>